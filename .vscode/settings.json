{
  "eslint.workingDirectories": ["backend", "frontend"],
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    }
  },
  "svelte.plugin.css.diagnostics.enable": false
}
